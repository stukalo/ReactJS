{
  "ecmaFeatures": {
    "jsx": true
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
	"no-unused-vars": 0,
	"no-nuderscore-dangle": 0,
	"no-alert": 0,
	"no-lone-blocks": 0,
    "no-undef": "error"
  },
  "globals": {
    "JQuery": true,
    "$": true
  }
}